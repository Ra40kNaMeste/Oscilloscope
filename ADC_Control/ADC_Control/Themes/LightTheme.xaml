<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ConditionStyle" TargetType="{x:Type Rectangle}">
        <Setter Property ="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
    </Style>
    <Style x:Key="delimiterLineStyle" TargetType="{x:Type Line}">
        <Setter Property="StrokeThickness" Value="2"/>
        <Setter Property="Stroke" Value="Black"/>
    </Style>
    <SolidColorBrush x:Key="TrueConditionBrush" Color="LightGreen"/>
    <SolidColorBrush x:Key="FalseConditionBrush" Color="Red"/>
    <SolidColorBrush x:Key="NullConditionBrush" Color="White"/>
    
</ResourceDictionary>